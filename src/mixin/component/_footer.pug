mixin Footer(item)
  .c-footer
    .c-footer__wrap
      .c-footer__content
        a.c-footer__link(href="https://www.instagram.com/cj1108_/") 
          i.c-footer__item.fa-brands.fa-instagram.u-title-xl
          p.c-footer__item.u-text-md.u-font-en #{item.name}
      .c-footer__content 
        p.c-footer__item.u-text-xxl.u-font-en.u-fw-800 Email 
        p.c-footer__item.u-text-md.u-font-en #{item.email}
      .c-footer__content 
        a.c-footer__link(href="https://codepen.io/CJIE_L") 
          i.c-footer__item.fa-brands.fa-codepen.u-title-xl
    .c-footer__wrap
      .c-footer__content
        a.c-footer__link(href="https://github.com/Liao-chen-jia")
          i.fa-brands.fa-github.u-title-xl
      .c-footer__content.u-ml-80
        i.c-footer__item.fa-brands.fa-discord.u-title-xl
        p.c-footer__item.u-text-md.u-font-en #{item.dcname}